       fhir:title [ fhir:v "HIV test is not required" ] ;
       fhir:description [ fhir:v "HIV test is not required" ] ;
       fhir:textEquivalent [ fhir:v "A test is not required if the woman is already confirmed HIV positive" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
