     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "1" ] ;
       fhir:title [ fhir:v "Conduct HIV test" ] ;
       fhir:description [ fhir:v "Conduct HIV test" ] ;
       fhir:textEquivalent [ fhir:v "In high-prevalence settings, provider-initiated testing and counselling (PITC) for HIV should be considered a routine component of the package of care for pregnant women in all ANC settings\n\nAn HIV test is required for all pregnant women at the first contact in pregnancy if the HIV prevalence in the pregnant woman population is 5% or higher." ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of HIV in pregnant women\" ≥ 5%)\n  AND (\"ANC contact number\" = 1)" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct HIV test" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "2" ] ;
       fhir:title [ fhir:v "Conduct HIV test" ] ;
       fhir:description [ fhir:v "Conduct HIV test 2" ] ;
       fhir:textEquivalent [ fhir:v "Another HIV test is required for all pregnant women at the first contact of the 3rd trimester (28 weeks), if the HIV prevalence in the pregnant woman population is 5% or higher" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of HIV in pregnant women\" ≥ 5%)\n  AND (\"Gestational age\" ≥ 29 weeks)\n  AND (\"HIV status\" = \"HIV negative\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct HIV test 2" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "3" ] ;
       fhir:title [ fhir:v "HIV test is optional" ] ;
       fhir:description [ fhir:v "HIV test is optional" ] ;
       fhir:textEquivalent [ fhir:v "In low-prevalence settings, provider-initiated testing and counselling (PITC) can be considered for pregnant women in ANC settings as a key component of the effort to eliminate mother-to-child transmission of HIV, and to integrate HIV testing with syphilis, viral or other key tests, as relevant to the setting, and to strengthen the underlying maternal and child health systems" ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "\"Population prevalence of HIV in pregnant women\" < 5%" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should HIV test is optional" ]
         ]
       ] )
