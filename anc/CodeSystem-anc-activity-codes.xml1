    <display value="Nutrition supplementation"/>
  </concept>
  <concept>
    <code value="ANC.B10.6"/>
    <display value="Risk reduction &amp; general counselling"/>
  </concept>
  <concept>
    <code value="ANC.B10.7"/>
    <display value="Immunizations"/>
  </concept>
  <concept>
    <code value="ANC.B10.8"/>
    <display value="Intimate partner violence first-line support and care"/>
  </concept>
  <concept>
    <code value="ANC.B10.9"/>
    <display value="Deworming &amp; malaria prophylaxis"/>
  </concept>
  <concept>
    <code value="ANC.B11"/>
    <display value="Urgent referral needed?"/>
    <definition value="If there are any diagnoses requiring referral or services that cannot be provided at the facility, health worker refers pregnant woman based on findings during counselling."/>
  </concept>
  <concept>
    <code value="ANC.B12"/>
    <display value="Scheduling"/>
    <definition value="If a referral is not required, health worker schedules follow-up contact."/>
  </concept>
  <concept>
    <code value="ANC.C"/>
    <display value="Referral"/>
    <definition value="An antenatal care referral to provide timely and appropriate referrals to a higher-level facility or health-care provider."/>
  </concept>
  <concept>
    <code value="ANC.C1"/>
    <display value="Emergency referral?"/>
