  <concept>
    <code value="ANC.C6"/>
    <display value="Provide information to referral facility"/>
  </concept>
  <concept>
    <code value="ANC.D"/>
    <display value="Antenatal health promotion, follow-up in the community"/>
    <definition value="The process followed to provide routine health promotion and follow-up within the community."/>
  </concept>
  <concept>
    <code value="ANC.E"/>
    <display value="Reporting on aggregate indicators"/>
    <definition value="The process followed to compile and submit relevant data contributing to indicators and other reporting needs on a routine basis."/>
  </concept>
  <concept>
    <code value="ANC.Confirugation"/>
    <display value="Configuration"/>
    <definition value="Configuration settings for decision logic and indicators that may vary by setting"/>
  </concept>
  <concept>
    <code value="ANC.End"/>
    <display value="End"/>
    <definition value="The activity of gathering information about the end of a pregnancy and closing an antenatal care record."/>
  </concept>
