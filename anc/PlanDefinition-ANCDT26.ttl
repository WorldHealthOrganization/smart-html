  fhir:action ( [
     fhir:title [ fhir:v "ANC.DT.26 Calcium and vitamin A supplementation" ] ;
     fhir:trigger ( [
       fhir:type [ fhir:v "named-event" ] ;
       fhir:name [ fhir:v "IF \"Population with low dietary calcium intake\" = TRUE AND/OR IF \"Population with vitamin A deficiency\" = TRUE" ]
     ] ) ;
     fhir:action ( [
       fhir:id [ fhir:v "1" ] ;
       fhir:title [ fhir:v "Provide calcium supplementation" ] ;
       fhir:description [ fhir:v "Provide calcium supplementation" ] ;
       fhir:textEquivalent [ fhir:v "In populations with low dietary calcium intake, daily calcium supplementation (1.5–2.0 g oral elemental calcium) is recommended for pregnant women to reduce the risk of pre-eclampsia. \n\nDietary counselling of pregnant women should promote adequate calcium intake through locally available, calcium-rich foods.\n\n– Divide the total dose into three doses, preferably taken at meal-time.\n– Iron and calcium should preferably be administered several hours apart rather than concomitantly.\n\nPlease refer to calcium sources listed below for additional guidance that can be provided. " ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): A.3. Calcium supplements (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "IF \"Population with low dietary calcium intake\" = TRUE" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Provide calcium supplementation" ]
         ]
       ] ) ;
       fhir:action ( [
         fhir:title [ fhir:v "Provide calcium supplementation" ]
       ] [
         fhir:title [ fhir:v "1.5 g ≤ \"Amount of calcium supplements provided\" ≤ 2 g " ]
       ] )
     ] [
       fhir:id [ fhir:v "2" ] ;
       fhir:title [ fhir:v "Do not provide calcium supplementation" ] ;
       fhir:description [ fhir:v "Do not provide calcium supplementation" ] ;
       fhir:textEquivalent [ fhir:v "Dietary counselling of pregnant women should promote adequate calcium intake through locally available, calcium-rich foods.\n\nPlease refer to calcium sources listed below for additional guidance that can be provided. " ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): A.3. Calcium supplements (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "IF \"Population with low dietary calcium intake\" = FALSE" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Do not provide calcium supplementation" ]
         ]
       ] )
     ] [
       fhir:id [ fhir:v "3" ] ;
       fhir:title [ fhir:v "Provide vitamin A supplementation" ] ;
       fhir:description [ fhir:v "Provide vitamin A supplementation" ] ;
       fhir:textEquivalent [ fhir:v "Give a dose of up to 10,000 IU vitamin A per day, or a weekly dose of up to 25,000 IU.\n\nA single dose of a vitamin A supplement greater than 25,000 IU is not recommended as its safety is uncertain. Furthermore, a single dose of a vitamin A supplement greater than 25,000 IU might be teratogenic if consumed between day 15 and day 60 from conception.\n\nPlease refer to vitaminA sources listed below for additional guidance that can be provided. " ] ;
       fhir:documentation ( [
         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): A.4. Vitamin A supplements (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "IF \"Population with vitamin A deficiency\" = TRUE" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Provide vitamin A supplementation" ]
         ]
       ] ) ;
       fhir:action ( [
         fhir:title [ fhir:v "Provide vitamin A supplementation" ]
       ] [
         fhir:title [ fhir:v "\"Amount of vitamin A supplementation provided\" ≤ 10 000 IU" ]
       ] [
         fhir:title [ fhir:v "Type of vitamin A supplement provided\" = \"Daily\"" ]
       ] )
