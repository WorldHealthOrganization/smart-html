         fhir:type [ fhir:v "citation" ] ;
         fhir:label [ fhir:v "WHO ANC recommendations (2016): B.1.7. Human immunodeficiency virus (HIV) and syphilis (3)" ]
       ] ) ;
       fhir:condition ( [
         fhir:kind [ fhir:v "applicability" ] ;
         fhir:expression [
           fhir:description [ fhir:v "(\"Population prevalence of syphilis in pregnant women\" ≥ 5%)\n  AND (\"Syphilis test type\" = \"Rapid syphilis test (RST)\")\n  AND (Syphilis test result\" = \"Syphilis positive\")" ] ;
           fhir:language [ fhir:v "text/cql-identifier" ] ;
           fhir:expression [ fhir:v "Should Conduct syphilis counselling and further testing" ]
         ]
       ] )
